<%- include("./partials/header") %>
<h1><%=title%></h1>
<table class="table table-sm">
    <% if (fields.length > 0) { %>
        <thead><tr>
            <% fields.forEach((field)=>{ %>
                <th><%=field%></th>
            <% }); %>
        </tr></thead>
    <% } %>
    <tbody>
    <% data.forEach((course)=>{ %>
        <tr>
            <% for (let key in course){ %>
                <td><%=course[key]%></td>
            <% } %>
        </tr>
    <% }) %>
    </tbody>
</table>
<%- include("./partials/footer") %>